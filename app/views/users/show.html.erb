<main>
<div class="row">
  <div class="input-field col s8 offset-s2">
<h4 class="cyan-text blue-grey-text">MyPage : <%= @mail %></h4>
  </div>
</div>
<% @content.each do |content| %>
  <div class="row">
    <div class="col s8 offset-s2">
      <div class="card">
        <div class="card-content">

            <p><%= content.content %></p>

        </div>
        <div class="card-action right-align">

                <%= link_to 'EDIT', "/blog/#{content.id}/edit", method: :get %>

                <%= link_to 'DELETE', "/blog/#{content.id}", method: :delete %>

        </div>
      </div>
    </div>
  </div>
<% end %>
</main>
  <%= paginate(@content) %>
